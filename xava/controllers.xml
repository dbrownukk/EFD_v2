<?xml version = "1.0" encoding = "ISO-8859-1"?>

<!DOCTYPE controllers SYSTEM "dtds/controllers.dtd">

<controllers>


	<object name="xava_activeSection" class="java.lang.Integer"
		value="0" />


	<controller name="EFD_v2">
		<extends controller="TypicalNotResetOnSave" />
	</controller>
	<!-- <controller name="EFD_v2"> <extends controller="Typical" /> </controller> -->


	<!-- <controller name="Project"> <extends controller="Typical"/> <action 
		name="save" mode="detail" class="org.openxava.actions.SaveAction" by-default="if-possible" 
		image="save.gif" icon="content-save" keystroke="Control S"> <set property="resetAfter" 
		value="false"/> </action> </controller> -->
	<controller name="Spreadsheet">
		<extends controller="TypicalRealExcel" />
		<action name="Template Spreadsheet" mode="detail" in-each-row="false"
			class="efd.actions.CreateXlsFileAction" />
	</controller>

	<controller name="ParseSpreadsheet">
		<extends controller="TypicalNotResetOnSave" />
		<action name="Parse Spreadsheet" label="Parse Interview Spreadsheet"
			icon="file-excel" mode="detail" in-each-row="false"
			class="efd.actions.ParseXLSFile" />
		<action name="Validate Asset Data" label="Validate Asset Data"
			icon="marker-check" mode="detail" in-each-row="false"
			class="efd.actions.ValidateXLSFile" />
	</controller>

	<controller name="Reports">
		<subcontroller controller="ReportsList" icon="menu" />
	</controller>

	<controller name="ReportsList">
		<action name="Country" label="Country Report" icon="view-list"
			class="efd.reports.ReportCountrys" />
			<action name="Projects" label="Project Report" icon="view-list"
			class="efd.reports.ReportProjects" />
			<action name="Sites By Country" label="Sites By Country" icon="view-list"
			class="efd.actions.RunRestReport" />
	</controller>





	<controller name="LivelihoodZone">
		<extends controller="EFD_v2" />
		<action name="filteredsearch" hidden="true" image="search.gif"
			class="efd.actions.FilteredLZ" />

	</controller>
	<!-- <extends controller="EFD_v2"/> <action name="new LZ" image="new.gif" 
		hidden="true" icon="library-plus" class="efd.actions.NewLZ"/> <action name="add 
		LZ" hidden="true" icon="table-row-plus-after" class="efd.actions.AddLZ"/> 
		<action name="filteredsearch" hidden="true" image="search.gif" class="efd.actions.FilteredLZ"/> 
		</controller> -->
	<controller name="Community">
		<extends controller="EFD_v2" />
		<action name="filteredSitesearch" hidden="true" image="search.gif"
			class="efd.actions.FileteredSite" />
	</controller>

	<controller name="Sections">
		<action name="change" class="efd.actions.AssetChangeSectionAction"
			keystroke="Control 1..9" />
	</controller>


	<controller name="WealthGroupInterview">
		<extends controller="EFD_v2" />
		<action name="resourceSubTypeFilter" hidden="false" image="search.gif"
			class="efd.actions.ResourceSubTypeFilter" />


		<action name="change" class="efd.actions.AssetChangeSectionAction"
			keystroke="Control 1..9">
			<use-object name="xava_activeSection" />
		</action>





	</controller>

</controllers>